{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "${command:pickArgs}"
        },
        {
            "name": "Python: Train with Distributed Data Parallel",
            "type": "debugpy",
            "request": "launch",
            "python": "${command:python.interpreterPath}",
            "module": "torch.distributed.launch",
            "args": [
                "--nproc_per_node=1",
                "--master_port=1234",
                "train.py",
                "--dist", "True",
                "--dataset_list", "PAOT_TEMP",
                "--datasetkey", "01" , "04", "05", "07", "08", "09", "10_03", "10_06", "10_07", "10_08", "10_09", "10_10",
                "--data_root_path", "/data/clip/ClipReady/",
                "--num_workers", "1",
                "--num_samples", "4",
                "--cache_dataset",
                "--cache_rate", "0.01",
                "--uniform_sample"
            ],
            "env": {
                "CUDA_VISIBLE_DEVICES": "0,1"
            },
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Python: No cacheTrain with Distributed Data Parallel",
            "type": "debugpy",
            "request": "launch",
            "python": "${command:python.interpreterPath}",
            "module": "torch.distributed.launch",
            "args": [
                "--nproc_per_node=1",
                "--master_port=1235",
                "train.py",
                "--dist", "True",
                "--dataset_list", "PAOT_TEMP",
                "--datasetkey", "01" , "04", "05", "07", "08", "09", "10_03", "10_06", "10_07", "10_08", "10_09", "10_10",
                "--data_root_path", "/data/clip/ClipReady/",
                "--num_workers", "1",
                "--num_samples", "4",
                "--uniform_sample",
            ],
            "env": {
                "CUDA_VISIBLE_DEVICES": "0"
            },
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "SuperVision",
            "type": "debugpy",
            "request": "launch",
            "python": "${command:python.interpreterPath}",
            "module": "torch.distributed.launch",
            "args": [
                "--nproc_per_node=1",
                "--master_port=1235",
                "train.py",
                "--dist", "True",
                "--dataset_list", "PAOT_TEMP",
                "--datasetkey", "01" , "04", "05", "07", "08", "09", "10_03", "10_06", "10_07", "10_08", "10_09", "10_10",
                "--data_root_path", "/data/clip/ClipReady/",
                "--num_workers", "1",
                "--num_samples", "4",
                "--uniform_sample",
                "--do_supervision" , "0.2"
            ],
            "env": {
                "CUDA_VISIBLE_DEVICES": "0"
            },
            "console": "integratedTerminal",
            "justMyCode": false
        }
    ]
}